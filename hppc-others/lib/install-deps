
mvn install:install-file \
    -DgroupId=trove \
    -DartifactId=trove \
    -Dversion=3.0.0rc1 \
    -Dfile=trove-3.0.0rc1.jar \
    -Dpackaging=jar -DgeneratePom=true

mvn install:install-file \
    -DgroupId=it.unimi.dsi \
    -DartifactId=fastutil \
    -Dversion=6.1.0 \
    -Dfile=fastutil-6.1.0.jar \
    -Dpackaging=jar -DgeneratePom=true
