
mvn install:install-file \
    -DgroupId=trove \
    -DartifactId=trove \
    -Dversion=3.0.0rc1 \
    -Dfile=trove-3.0.0rc1.jar \
    -Dpackaging=jar -DgeneratePom=true

mvn install:install-file \
    -DgroupId=com.google.code.java-allocation-instrumenter \
    -DartifactId=java-allocation-instrumenter \
    -Dversion=2.0 \
    -Dfile=allocation.jar \
    -Dpackaging=jar \
    -DgeneratePom=true